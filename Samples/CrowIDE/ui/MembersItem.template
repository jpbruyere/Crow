		<Label Style="MemberViewLabel" Text="{Name}" Foreground = "{LabForeground}" />
		<CheckBox Background="White" Foreground = "{LabForeground}" Height="Stretched" Caption="" IsChecked="{²Value}"/>
	</HorizontalStack>
</ItemTemplate>
